public class AocDay1Test {

    public static void main(){
        
        StaticResource sr = [SELECT Id, Body FROM StaticResource WHERE Name = 'AocDay1Input' LIMIT 1];
        String body = sr.Body.toString();
        
        List<String> StringNums = body.split('\\n');
        List<Integer> nums = new List<Integer>();
        Integer counter = 0;
             
        for (String s : StringNums){
            nums.add(Integer.valueOf(s));
        }
    
        for (integer i=0; i<nums.size()-1; i++) {
            if (nums[i]<nums[i+1]) {
            	counter++;
            }
        }
        System.debug('Answer : '+counter);
    }
}
